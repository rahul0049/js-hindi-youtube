const marval_heros=["thor","Ironman","spiderman"]
const dc_heros=["superman","flash","batman"]
// marval_heros.push(dc_heros) // it will push dc_heros as an element in the last 
// console.log(marval_heros)
// console.log(marval_heros[3][1])

//so correct way is concat which returns a new array so we need to store it in a new variable
const allHeros=marval_heros.concat(dc_heros)
console.log(allHeros);

// another way is spread operator
const allNewHeros=[...marval_heros,...dc_heros]
console.log(allNewHeros)

const another_array=[1,2,3,[4,5,6],7,[6,7,[4,5]]]
const real_another_array=another_array.flat(Infinity) // where 3 is number of depth
console.log(real_another_array)

Array.isArray("Hitesh")
Array.from("Hitesh")

// converting array from values 
let score1=100
let score2=200
let score3=300
console.log(Array.of(score1,score2,score3))
